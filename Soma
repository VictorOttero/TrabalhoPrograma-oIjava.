public class CalcularSoma {
    public static void main(String[] args) {
        int[] vetor = {5, 10, 15, 20, 25}; // Vetor de exemplo
        
        int soma = 0;
        for (int i = 0; i < vetor.length; i++) {
            soma += vetor[i];
        }
        
        System.out.println("A soma dos elementos do vetor Ã©: " + soma);
    }
}
